<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>servlet.AdminServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DataFormServlet</servlet-name>
    <servlet-class>servlet.DataFormServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserServlet</servlet-name>
  	<servlet-class>servlet.UserServlet</servlet-class>
</servlet>
	<servlet>
  	<servlet-name>HomeServlet</servlet-name>
    <servlet-class>servlet.HomeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>servlet.LoginServlet</servlet-class>
  </servlet>
   <servlet>
  	<servlet-name>SignupServlet</servlet-name>
    <servlet-class>servlet.SignupServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GardenServlet</servlet-name>
    <servlet-class>servlet.GardenServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SearchServlet</servlet-name>
    <servlet-class>servlet.SearchServlet</servlet-class>
  </servlet>
   <servlet>
  	<servlet-name>EditUserServlet</servlet-name>
    <servlet-class>servlet.EditUserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditUserPortraitServlet</servlet-name>
    <servlet-class>servlet.EditUserPortraitServlet</servlet-class>
  </servlet>
   <servlet>
  	<servlet-name>EditGardenServlet</servlet-name>
    <servlet-class>servlet.EditGardenServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreateGardenServlet</servlet-name>
    <servlet-class>servlet.CreateGardenServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MemberServlet</servlet-name>
    <servlet-class>servlet.MemberServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UploadServlet</servlet-name>
    <servlet-class>servlet.UploadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PortraitUploadServlet</servlet-name>
    <servlet-class>servlet.PortraitUploadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DownloadServlet</servlet-name>
    <servlet-class>servlet.DownloadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DocumentServlet</servlet-name>
    <servlet-class>servlet.DocumentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>pollVisitsByCountyServlet</servlet-name>
    <servlet-class>servlet.pollVisitsByCountyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>pollVisitsByPlantCoreopsisServlet</servlet-name>
    <servlet-class>servlet.pollVisitsByPlantCoreopsisServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>pollVisitsByPlantMonardaServlet</servlet-name>
    <servlet-class>servlet.pollVisitsByPlantMonardaServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>pollVisitsByPollTypeServlet</servlet-name>
    <servlet-class>servlet.pollVisitsByPollTypeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ChartDirectoryServlet</servlet-name>
    <servlet-class>servlet.ChartDirectoryServlet</servlet-class>
  </servlet>

  <!-- Servlet mappings -->
  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/admin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DataFormServlet</servlet-name>
    <url-pattern>/dataForm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserServlet</servlet-name>
  	<url-pattern>/user</url-pattern>
  </servlet-mapping>
	<servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SignupServlet</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GardenServlet</servlet-name>
    <url-pattern>/garden</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchServlet</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditUserServlet</servlet-name>
    <url-pattern>/editUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditGardenServlet</servlet-name>
    <url-pattern>/editGarden</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>CreateGardenServlet</servlet-name>
    <url-pattern>/createGarden</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MemberServlet</servlet-name>
    <url-pattern>/members</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UploadServlet</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PortraitUploadServlet</servlet-name>
    <url-pattern>/portraitUpload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownloadServlet</servlet-name>
    <url-pattern>/download</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditUserPortraitServlet</servlet-name>
    <url-pattern>/editUserPortrait</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DocumentServlet</servlet-name>
    <url-pattern>/documents</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>pollVisitsByCountyServlet</servlet-name>
    <url-pattern>/pollVisits1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>pollVisitsByPlantCoreopsisServlet</servlet-name>
    <url-pattern>/pollVisits2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>pollVisitsByPlantMonardaServlet</servlet-name>
    <url-pattern>/pollVisits3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>pollVisitsByPollTypeServlet</servlet-name>
    <url-pattern>/pollVisits4</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ChartDirectoryServlet</servlet-name>
    <url-pattern>/chartDirectory</url-pattern>
  </servlet-mapping>
</web-app>
